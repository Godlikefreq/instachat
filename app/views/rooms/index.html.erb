<h1>Users online</h1>
<div id="users-list">
  <% @users.each do |user| %>
    <%= render partial: "users/user", locals: { user: } %>
  <% end %>
</div>

<h1>Rooms</h1>

<%= turbo_stream_from "rooms" %>
<div id="rooms" class="mt-4 list-unstyled">
  <%= render @rooms %>
</div>

<div class="mt-2">
  <%= render 'rooms/form', room: @room %>
</div>
